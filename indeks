for i in 'python':
    print(i)

#indeks selalu di awali oleh 0 bukan 1
# %d masuk ke i+1
# %s masuk ke daftar [i]
daftar = ['hafizh', 'eben', "edmund", 'zikri', 'nabil', 'nourma', 'dea', 'vanya']
for i in range (len(daftar)):
    print ('%d:%s' % (i+1, daftar[i]))

#1 awal, 6 akhir, 2 step
for i in range(1,6,2):
    print('%d: hello world' % i)

for i in range(6,0,-2):
    print('%d: hello world' % i)

i=int(input("x ="))

while i < 123:
    print(i, end = ',')
    i += 1





